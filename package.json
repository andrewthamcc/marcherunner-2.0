  "bugs": {
    "url": "https://github.com/andrewthamcc/marche_runner/issues"
  },
  "homepage": "https://github.com/andrewthamcc/marche_runner#readme",
  "devDependencies": {
    "concurrently": "^6.0.0"
  }
}
